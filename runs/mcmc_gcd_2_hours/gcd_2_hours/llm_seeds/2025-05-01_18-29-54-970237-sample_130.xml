<?xml version = 
1.0 
encoding = 
"ISO-8859-1" 
standalone = 
	no?>

<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:s="http://www.w3.org/2000/01/xmlschema#"
         xsi:schemaLocation="http://www.w3.org/2000/01/xmlschema#
         schema.xsd"
         xmlns="http://www.w3.org/1999/XMLSchema"
         targetNamespace="http://www.w3.org/2004/xmlns"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns="http://www.w3.org/2004/xmlns"
         version="1.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema" 
         xmlns:tns="http://www.example.com/catalog">
    <xs:schema xmlns:xs = "http://www.w3.org/2001/XMLSchema" 
               targetNamespace ="
               xsd"
               xmlns="http://www.w3.org/1999/XMLSchema"
               elementFormDefault="qualified"
               xmlns:xsi="
               "http:www.w3.org2001XSTypeschemaInstancemetadataXMLSchema1
                ="
               xsi="http:www.w3.org2001XSTypeschemaInstancemetadataXMLSchema1
                ="
               xmlns:tns="
                http:www.example.comcatalog
                ="
               xmlns:xsi =
               http:www.w3.org2001XSTypeschemaInstancemetadataXMLSchema1
               3  ">
         <xs:element name="book">
           <xs:complexType>
             <xs:sequence>
               <xs:element name="author"
                 type="xs:string"/>
               <xs:element name="title" type="xs:string"/>
             </xs:sequence>
           </xs:complexType>
         </xs:element>
         <xs:element name="bookstore"/>
         <xs:element name="genre"
             type="xs:string"/>
         <xs:element name="price" 
             type="xs:decimal"/>       
    </xs:schema>
    <xs:include schemaLocation="catalog.xsd" 
                 src="catalog.xsd"/>
  </catalog>