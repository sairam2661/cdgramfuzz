  <Solution2>
  <?xml version="1.0" encoding="UTF-8"?>
  <!-- 
  Document Type Definition for Library Catalog
  -->
  <!---->
  <element-library-catalog>
    <DTD name="library-catolog.dtd">
      <!--
         The element catalog is the root element of the document.
        It has the attributes catalog_name and description.
      -->
      <element-catalog>
        <attribute name="catalog_name"/>
        <attribute name="description"/>
        <element-book>
            <attribute name="title"/>
            <attribute name="author"/>
            <attribute name="publication-date"/>
            <attribute name="genre"/>
        </element-book>
      </element-catalog>
    </DTD>
  </element-library-catalog>
  </Solution2> 

