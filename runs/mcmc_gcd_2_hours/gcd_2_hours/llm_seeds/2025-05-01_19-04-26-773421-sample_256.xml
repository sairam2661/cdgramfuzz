         xmlns:jaxrs="http://jax.rs.annotation.handler"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://jax.rs.annotation.handler
         http://java.sun.com/jsr-311 xmlNS="http:jax.rs.annotation.handler
         ="/">
    <cxf:jaxrs provider="cxf-jaxrs-provider"></cxf:jaxrs>
    <bean id="resource" class="com.example.Resource">
      <cxf:property name="url" value="http://localhost:8080/service"></cxf:property>
    </bean>
  </beans>
</cxf:annotation-driven> 

