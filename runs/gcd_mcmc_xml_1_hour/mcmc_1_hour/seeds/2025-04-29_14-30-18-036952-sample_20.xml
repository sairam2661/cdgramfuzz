<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<catalog xmlns:prod="http://example.com/products"
          xmlns:mov="http://example.com/movies"
          xmlns:cater="http://example.com/cafes"
          xmlns:tel="http://example.com/telecom"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://example.com/products products.xsd
                              http://example.com/movies movies.xsd
                              http://example.com/cafes cafes.xsd
                              http://example.com/telecom telecom.xsd">

  <!-- Product -->
  <prod:item name="Product A">
    <prod:description>This is a test product</prod:description>
    <prod:price currency="USD">9.99</prod:price>
    <prod:stock level="100"/>
  </prod:item>

  <!-- Movie -->
  <mov:movie name="Inception">
    <mov:genres>action/sci-fi</mov:genres>
    <mov:director>Christopher Nolan</mov:director>
    <mov:year>2010</mov:year>
  </mov:movie>

  <!-- Cafe -->
  <cater:location city="San Francisco" country="USA">
    <cater:address street="123 Main St" zipcode="94105"/>
  </cater:location>

  <!-- Telecom -->
  <tel:phone number="512-123-4567" type="mobile"/>
  <tel:address street="456 Elm St" zipcode="78701"/>
  <tel:fax number="512-123-5678" type="office"/>

</catalog>