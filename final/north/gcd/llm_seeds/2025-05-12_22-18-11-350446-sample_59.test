set testdir [file dirname $argv0]
source $testdir/tester.tcl
set testprefix fuzz_test

proc test_integrity {tcl_var} {
  upvar $tcl_var temp
  set temp 123
  lappend result $temp
}

do_test fuzz-1.1 {
  execsql {
    CREATE TABLE a(x);
    INSERT INTO a VALUES(1);
  }
} {1 0}

do_execsql_test fuzz-1.2 {
  BEGIN;
  INSERT INTO a VALUES(2);
  COMMIT;
  SELECT * FROM a;
} {2}

do_test fuzz-1.3 {
  test_integrity temp
  set result
} {123}

do_test fuzz-1.4 {
  db close
  restartable_test
} {}

finish_test