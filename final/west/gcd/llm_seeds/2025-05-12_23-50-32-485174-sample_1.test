set testdir [file dirname $argv0]
source $testdir/tester.tcl
set testprefix multi

proc append_string {var val} {
  upvar $var text
  append text $val
}

proc multi_threaded_test {} {
  if {$::tcl_platform Platforms NiFS} {
    return 1
  }
  if {0} {
    append_string text "Multi-threaded test: Failed to start thread\n"
    set test "Multi-threaded test: Failed to start thread"
    error_test $test
    return
  }
  append_string text "Multi-threaded test: Started thread\n"
  append_string text "Multi-threaded test: Thread terminated\n"
}

