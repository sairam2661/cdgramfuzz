{
  "commands": [
    {
      "cmd": "git add.",
      "args": ["--all"],
      "description": "Stage all changes"
    },
    {
      "cmd": "git commit",
      "args": ["-m", "Initial commit"],
      "description": "Commit changes"
    }
 ,
