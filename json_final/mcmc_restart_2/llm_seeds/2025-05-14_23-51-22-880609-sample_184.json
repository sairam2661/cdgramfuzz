    "start": "npm run build && webpack-dev-server --mode development"
  },
  "dependencies": {
    "webpack": "^5.60.0",
    "webpack-dev-server": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10"
  },
  "author": "John Doe"
}