{
  "commands": [
    {
      "cmd": "git add.",
      "args": ["-A"],
      "status": true
    },
    {
      "cmd": "git commit",
      "args": ["-m", "Initial commit"],
      "status": false
    }
 ,
    {
      "cmd": "npm install",
      "args": ["--yes"],
      "status": true
    }
