<?xml version="1.0" encoding="ISO-8859-5"?>
<!DOCTYPE document [
  <!ELEMENT document (html, metadata, section*)>
  <!ELEMENT html (#PCDATA|a|b|i|em|span)*>
  <!ELEMENT metadata (#PCDATA|author|date|version)*>
  <!ATTLIST document xmlns:fb CDATA #IMPLIED>
]>
<document xmlns:fb="http://www.facebook.com/2008/fbml"
          fb:app_id="1234567890" fb:version="3.2">
  <html>
    <head>
      <title>Complex Fuzz Test</title>
    </head>
    <body>
      <section>
        <span style="font-size: 16pt; font-weight: bold">Some text</span>
        <a href="http://www.example.com">Example link</a>
        <em>Emphasized text</em>
        <b>Bold text</b>
        <i>Italic text</i>
      </section>
    </body>
  </html>
  <metadata>
    <author>John Doe</author>
    <date>2020-10-15</date>
    <version>1.0</version>
  </metadata>
  <section>
    <paragraph>This is a paragraph with a footnote: <fn>This is a footnote.</fn></paragraph>
  </section>
</document>