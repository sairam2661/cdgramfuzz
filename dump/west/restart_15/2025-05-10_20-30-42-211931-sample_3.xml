<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "fruits.dtd" [
  <!ENTITY note "Note">
  <!ENTITY entity "&entity;">
]>
<document>
  <html xmlns="http://www.w3.org/1999/xhtml"
        xmlns:html2="http://www.example.com/xmlns/html2"
        xml:lang="en-US">
    <head>
      <title>Example of Nested Lists</title>
      <meta name="author" content="John" />
    </head>
    <body>
      <section>
        <h1>Top-Level Element</h1>
        <ul>
          <li>Item 1</li>
          <ul>
            <li>Inner List Item 1</li>
            <li>Inner List Item 2</li>
            <ul>
              <li>Even More Nested</li>
            </ul>
          </ul>
        </ul>
      </section>
      <section>
        <p><![CDATA[
          &note; &#13; This is a paragraph with a line feed.
        ]]></p>
      </section>
    </body>
  </html>
  <metadata>
    <author>
      <name>John Doe</name>
      <contact>
        <email>test@example.com</email>
      </contact>
    </author>
    <date>2023-01-01</date>
  </metadata>
</document>