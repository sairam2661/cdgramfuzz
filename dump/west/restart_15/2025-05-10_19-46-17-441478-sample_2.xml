<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE document [
  <!ATTLIST document
    id ID #REQUIRED
    lang CDATA #IMPLIED
    xml:lang CDATA #IMPLIED>
]>
<document id="root"
    lang="en"
    xml:lang="en-US">
  <html>
  <head>
    <title>The Document Title</title>
    <meta name="charset" content="ISO-8859-1" />
  </head>
  <body>
    This is a long paragraph that will wrap at the end of the line.
  </body>
  </html>
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     targetNamespace="urn:foo"
     xmlns="urn:foo"
     xml:lang="en"
     elementFormDefault="qualified"
     attributeFormDefault="unqualified">
    <xs:element name="bar">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="baz" type="xs:integer" />
          <xs:element name="qux" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
  </xs:schema>
  <bar>
    <baz>42</baz>
    <qux>quuuuuux!</qux>
  </bar>
</document>