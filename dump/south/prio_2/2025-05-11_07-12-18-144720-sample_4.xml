<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE document PUBLIC "-//W3C//DTD MathML 2.0//EN"
  "http://www.w3.org/Math/DTD/mathml2/mathml2.dtd"
  [
    <!ENTITY percent "%">
  ]
>
<document>
  <html>
    <head>
      <title>Using XSLT to Generate MathML Tables and 3D Graphs</title>
      <meta name="keywords" content="XSLT MathML 3D graphics tables"/>
      <meta name="viewport" content="width=device-width, user-scalable=no"/>
    </head>
    <body>
      <table xmlns="urn:oasis:names:tc:opendocument:xmlns:table:1.0"
             xmlns:style="urn:oasis:names:tc:opendocument:xmlns:style:1.0"
             xmlns:office="urn:oasis:names:tc:opendocument:xmlns:office:1.0"
             office:version="1.0"
             office:master-page-reference="page0">
        <style:style-set office:auto-update="false"/>
      </table>
    </body>
  </html>
  <namespace
    href="http://example.org/namespace"
    xmlns:ns="http://example.org/namespace">
    <sub-namespace
      href="http://example.org/sub-namespace"
      xmlns:sub-ns="http://example.org/sub-namespace">Test</sub-namespace>
  </namespace>
  <comment>This is a comment.</comment>
  <pi target="http://www.w3.org/XML/1998/namespace">Test</pi>
  <section>
    <equation xmlns="http://www.w3.org/1998/Math/MathML"><mrow><mfrac><mrow><mn>1</mn><mi mathvariant="normal">x</mi></mrow><mrow><mrow><mn>2</mn></mrow></mrow></mfrac><mi mathvariant="normal">x</mi></mrow></equation>
  </section>
  <metadata>
    <author>Jane Doe</author>
    <date>2025-05-10</date>
    <pi target="http://www.w3.org/XML/1998/namespace" xml:space="preserve">   Test
</pi>
  </metadata>
</document>