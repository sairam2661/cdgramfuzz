<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE document [
  <!ELEMENT document (html*, metadata+, section+)>
  <!ELEMENT html EMPTY>
  <!ELEMENT metadata (author, date)>
  <!ELEMENT section (paragraph, sidebar*)>
  <!ELEMENT paragraph (#PCDATA | entity | element)*>
  <!ELEMENT sidebar (heading, block)>
  <!ELEMENT heading (#PCDATA | heading)*>
  <!ELEMENT block (#PCDATA | block)*>
  <!ELEMENT author (#PCDATA | author)*>
  <!ELEMENT date (#PCDATA | date)*>
  <!ENTITY note "Note">
]>
<document>
  <html xmlns:fb="http://www.facebook.com/2008/products/fbml"
        xmlns="http://www.w3.org/1999/xhtml"
        xml:lang="en-GB"
        xmlns:fbxml="http://www.facebook.comxfbml">
    <head profile="http://gizmo.com">
      <title>XML Fuzz Test</title>
      <meta name="keywords" content="fuzz, test, xml" />
      <style type="text/css" media="screen">
        <![CDATA[@import "http://example.com/styles.css";
        * { background-color: #00FF00; }
      ]]></style>
    </head>
  <body>
      <section xmlns:j="http://www.jb.com/xmlns/jb">
        <paragraph>
          Welcome to the XML Fuzz Test!
          <fb:xfbml height="250" url="http://example.com/xfbml.xml"></fb:xfbml>
          This is a test of the XML parser's abilities.
        </paragraph>
      </section>
  </body>
      </html>
  <metadata>
    <author>
      John Doe
      <address>
        123 Main St
        Apt 101
        Springfield, USA
      </address>
    </author>
    <date>2024-06-25</date>
  </metadata>
  <section>
      <paragraph>Here is another paragraph.</paragraph>
  </section>
  <section>
      <sidebar>
        <heading>Some Heading</heading>
        <block>
          This is some block-level content.
        </block>
      </sidebar>
  </section>
</document>