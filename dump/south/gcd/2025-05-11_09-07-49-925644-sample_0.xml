<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE document SYSTEM "complex.dtd" [
  <!ENTITY fruit "Apple">
  <!ELEMENT document ANY>
]>
<document xmlns="http://example.com/xmlns"
          xmlns:xlink="http://www.w3.org/1999/xlink"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="complex.xsd"
          xml:lang="fr"
          xml:space="preserve">
  <html lang="fr">
    <head>
      <title>Xml Complex Example</title>
      <meta charset="ISO-8859-1"/>
      <style type="text/css">
        <![CDATA[body { background-color: purple; }
                h1 { font-weight: bold; }]]>
      </style>
    </head>
    <body>
      <p>&fruit;</p>
      <section>
        <aside>
          <nav>
            <ol xmlns:xlink="http://www.w3.org/1999/xlink"
              xlink:href="link">
              <li>item 1</li>
              <li>item 2</li>
            </ol>
          </nav>
        </aside>
      </section>
    </body>
</html>
</document>