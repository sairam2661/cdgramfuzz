<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "example.dtd" [
  <!ENTITY fruit "Apple">
  <!ENTITY amp "&amp;">
  <!ENTITY lt "&lt;">
  <!ENTITY gt "&gt;">
  <!ENTITY quot "&quot;">
  <!ELEMENT example ANY>
]>
<document version="1.0" standalone="yes">
  <html xmlns:g="http://example.com" xml:lang="en">
    <head>
      <title>Complex Document</title>
      <meta name="description" content="A sample XML document" />
    </head>
    <body>
      <g:example>
        <metadata>
          <attribute xmlns:f="http://example.com/attr">f:fruit</attribute>
        </metadata>
        <item>f:fruit</item>
        <item> &amp; <i>&lt;</i> &note; &gt; &quot; &amp; </item>
      </g:example>
    </body>
  </html>
</document>