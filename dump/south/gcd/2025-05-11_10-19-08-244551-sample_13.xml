<?xml version="1.0" encoding="UTF-16"?>
<!DOCTYPE document SYSTEM "http://www.example.com/schema.dtd" [
  <!ENTITY % general "%general;">
  <!ELEMENT sample (#PCDATA|sample)*>
  <!ENTITY note "Note">
  <!NOTATION svg PUBLIC "-//W3C//DTD SVG 1.1//EN" "http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd">
]>
<document xmlns="http://www.w3.org/1999/xhtml" xmlns:xlink="http://www.w3.org/1999/xlink"
          xml:lang="en-US" xml:space="preserve" xmlns:svg="http://www.w3.org/2000/svg">
  <html>
    <head>
      <title>Test Document</title>
      <meta name="description" content="A test document" />
      <style><![CDATA[
        @import "style.css";
        body { font-family: Georgia, serif; }
        h1 { color: #FFFF00; }
      ]]></style>
    </head>
  <body>
    <text>Some <b>bold</b> text with <italic>italic</italic> and <emphasis>emphasis</emphasis>.</text>
    <annotation xml:lang="fr">
      <description>Cette est une annotation avec un langage diff√©rent.</description>
    </annotation>
    <comment>Comment: &note;</comment>
    <section>
      <figure>
        <graphic role="icon" xlink:href="icon.jpg"/>
        <!--  Note that this is an empty SVG element but a NOTATION is declared for SVG -->
        <inline svg:svg="test.svg">
          <svg:circle cx="50%" cy="50%" r="30" />
        </inline>
        <caption>graphic caption text.</caption>
      </figure>
    </section>
  </body>
  </html>
  <metadata>
    <!--  An element that must appear only once -->
    <!--  But can have some content, like attributes above -->
    <metadata xmlns="">
      <author>Jane Doe</author>
      <date>2024-05-10</date>
      <generator>My Generator</generator>
    </metadata>
  </metadata>
</document>