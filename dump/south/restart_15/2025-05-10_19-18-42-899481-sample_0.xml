<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document [
  <!ELEMENT document (html, metadata*, section+)>
  <!ELEMENT html (head, body)>
  <!ATTLIST head attr1 CDATA #IMPLIED>
  <!ELEMENT head (#PCDATA|title|meta|style)*>
  <!ELEMENT body (#PCDATA|h1|h2)*>
  <!ELEMENT metadata (author, date)>
  <!ELEMENT section (#PCDATA)*>
]>
<document>
  <html>
    <head attr1="value">
      <title>Complex Document</title>
    </head>
    <body>
      <h1>Fruits</h1>
      <h2>Nuts</h2>
    </body>
  </html>
  <metadata>
    <author>John Smith</author>
    <date>2024-07-26</date>
  </metadata>
  <section>This is a complex document.</section>
</document>